function synthPi = MonteCircle(nTries)
    inside = 0;
    outside = 0;
    for i = 1:nTries
        x = rand;
        y = rand;
        if ((x^2 + y^2) > 1)
            outside = outside + 1;
        else
            inside = inside + 1;
        end
    end
    synthpi = (4.*inside)./(inside + outside);
    